{
  "compilerOptions": {
    "declaration": true,
    "esModuleInterop": true,
    "outDir": "./dist",
    "sourceMap": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "lib": ["ES2022"],
    "module": "ES2022",
    "moduleResolution": "node",
    "target": "ES2022",
    
  },
  "exclude": ["./node_modules/", "./dist/"]
}
